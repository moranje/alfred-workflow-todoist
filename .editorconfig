# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Indentation override for all JS
[**.js]
indent_style = space
indent_size = 2

# Indentation override for all JS
[**.ts]
indent_style = space
indent_size = 2
